{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS Debug",
        "skipFiles": ["<node_internals>/**"],
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "start:dev"],
        "cwd": "${workspaceFolder}",
        "envFile": "${workspaceFolder}/backend/.env",
        "autoAttachChildProcesses": true
      }
    ]
  }
  